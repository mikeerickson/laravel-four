
<script type="text/javascript" src="/js/jquery-1.8.0.js"></script>
<h2>{{ now|date('m.d.Y') }} {{ now|date('h:i A') }}</h2>


Hello {{ fname }} {{ lname }} form twig template, reporting for duty!  
<p>

{% for item in kids %}
    <li>{{ loop.index }}: {{ item|title }}</li>
{% endfor %}

<h3>Users:</h3>

<table width="80%">
{% for user in users %}
	<tr>
    <td>{{ user.active }}</td>
    <td>{{ user.username|title }}</td>
    <td>{{ user.email }}</td>
    <td>{{ user.category|title }}</td>
    <td>{{ user.created_at|date('m.d.Y - h:i a') }}</td>
    <td>{{ user.updated_at|date('m.d.Y - h:i a') }}</td>
	</tr>
{% endfor %}
</table>

</p>

<p>

{% filter upper %}
	{{ kids|join(', ') }}
{% endfilter %}

</p>


<p>

{{ kids|join(' - ')|upper }}

</p>

<p>

{% filter title %}
	{{ family.father }} <br />
	{{ family.mother }} <br />
	{{ family.daughter1 }} <br />
	{{ family.son1 }} <br />
	{{ family.daughter2 }} <br />
	{{ family.son2 }} <br />
{% endfilter %}

</p>

<p>

{{ family|join('<br>') }}<br/>
{{ dogs|join('<br>') }}


</p>

<p>

{% set full = fname %}

{{ full|upper }} <br />

Fullname: {{ fname|title }} {{ lname|upper }} 

</p>

<div id="myForm">
	{{ form_open() }}
	{{ form_label('test','Test:') }}
	<input type='text' value='{{fname}}' />
	{{ form_close() }}
</div>


<p>
Developed by: {{ designer|title }}
</p>
